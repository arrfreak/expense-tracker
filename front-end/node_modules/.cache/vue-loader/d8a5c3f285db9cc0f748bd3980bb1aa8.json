 [\n          {\n            label: \"Actual\",\n            backgroundColor: [\"#41B883\", \"#E46651\"],\n            borderWidth: 1,\n            data: this.data\n          },\n\n          {\n            label: \"Target\",\n            backgroundColor: [\"#E46651\", \"#E46651\"],\n            borderWidth: 1,\n            data: [3600]\n          }\n        ]\n      };\n    },\n    getBankSplits() {\n      axios\n        .get(\"http://localhost:5000/getBankSplits\")\n        .then(response => {\n          var results = response.data;\n          this.data = [results.Total[0].MonthlyTotal, 3600];\n          this.fillData();\n        })\n        .catch(err => console.error(err));\n    }\n  },\n  computed: {\n    myStyles() {\n      return {\n        height: \"40%\",\n        width: \"85%\",\n        position: \"relative\"\n      };\n    }\n  }\n};\n</script>\n\n<style scoped></style>\n"]}]}