tbl\" align=\"center\">\n      <tr>\n        <td style=\"padding:10px;\">Description</td>\n        <td style=\"padding:10px;\">Card</td>\n        <td style=\"padding:10px;\">Category</td>\n        <td style=\"padding:10px;\">Date</td>\n      </tr>\n      <tr>\n        <td>\n          <input type=\"text\" v-model=\"description\" />\n        </td>\n        <td>\n          <input type=\"text\" v-model=\"card\" />\n        </td>\n        <td>\n          <input type=\"text\" v-model=\"category\" />\n        </td>\n        <td>\n          <input type=\"text\" v-model=\"date\" />\n        </td>\n        <td>\n          <button type=\"submit\" v-on:click=\"addItem()\">Submit</button>\n        </td>\n      </tr>\n      <!--v-for is how we loop through the response to print e recurring attribute. -->\n      <tr v-for=\"item in initData.Expenses\" :key=\"item.id\">\n        <td class=\"exp-col\">{{ item.description }}</td>\n        <td class=\"exp-col\">{{ item.card }}</td>\n        <td class=\"exp-col\">{{ item.category }}</td>\n        <td class=\"exp-col\">{{ item.date }}</td>\n      </tr>\n    </table>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\naxios.defaults.headers.common[\"Access-Control-Allow-Origin\"] = \"*\";\nconst baseURL = \"http://localhost:5000/addExpense\";\nexport default {\n  name: \"HelloWorld\",\n  props: [\"initData\"],\n  data() {\n    return {\n      myData: [],\n      description: \"\",\n      category: \"\",\n      card: \"\",\n      date: \"\"\n    };\n  },\n  methods: {\n    addItem() {\n      axios\n        .post(baseURL, {\n          date: this.date,\n          description: this.description,\n          card: this.card,\n          category: this.category\n        })\n        .then(response => {\n          this.initData = response.data;\n          this.$emit(\"ExpenseAdded\", response.data);\n        })\n        .catch(err => console.error(err));\n      //this.name = \"\";\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.tbl {\n  color: black;\n  border: 1 px blueviolet;\n}\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n\n.exp-col {\n  background-color: lightgray;\n  padding: 10px;\n}\n\n.button {\n  border: none;\n  border-radius: 2px;\n}\n\n.button-close {\n  background-color: red;\n}\n</style>\n"],"sourceRoot":"src/components"}]}